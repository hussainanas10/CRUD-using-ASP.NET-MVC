@model demo.Model.Employe_de


@{
    Layout = null;
}

<h5 class=" text-dark text-center">View Information </h5>

    @Html.HiddenFor(a => a.ID)
<div class="modal-content">
    <div class="modal-body">
        <div class="form-group">
            <label class="font-weight-bold">Name :</label>
            <p>@Model.Name</p>
        </div>

        <div class="form-group">
            <label class="font-weight-bold">Salary :</label>
            <p>@Model.Salary</p>
        </div>

        <div class="form-group">
            <label class="font-weight-bold">EmployID :</label>
            <p>@Model.EmployeID</p>
        </div>

        <div class="form-group">
            <label class="font-weight-bold">Grade :</label>
            <p>@Model.Grade</p>
        </div>
        
    </div>
    </div>


