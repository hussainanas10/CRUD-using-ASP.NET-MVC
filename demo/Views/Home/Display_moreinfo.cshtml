@model demo.Model.Employe_de


@{
    Layout = null;
}

<h5 class="border bg-dark text-white text-center">VIEW INFORMATION </h5>

    @Html.HiddenFor(a => a.ID)
    <div class="form-group">
        <label class="font-weight-bold">First Name :</label>
        <p>@Model.Name</p>
    </div>

    <div class="form-group">
        <label class="font-weight-bold">Last Name :</label>
        <p>@Model.Salary</p>
    </div>

    <div class="form-group">
        <label class="font-weight-bold">EmployID :</label>
        <p>@Model.EmployeID</p>
    </div>

    <div class="form-group">
        <label class="font-weight-bold">City :</label>
        <p>@Model.Grade</p>
    </div>

    

